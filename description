<!-- Кнопка показать выбор -->
<button type="button" onclick="displayRadioValue()"> Сформировать описание </button>
 
<br><br>
 <!-- Скрипт выбора -->
<script>
        function displayRadioValue() {
            document.getElementById("rezult").innerHTML = "";
            var ele = document.getElementsByTagName('input');
				for(i = 0; i < ele.length; i++) {
                    if(ele[i].type="radio") {
                    if(ele[i].checked)				
					document.getElementById("rezult").innerHTML  += ele[i].value + " "; 
					}
            }
        }
</script>


<!-- Внешний вид вывода описания -->
<style> #rezult {
		font-family: Arial;
		font-weight: bold;
		font-size: 16px;
		color: red; 
		
		width: 600px;
		height: 150px;
		border: 1px solid black;
		margin-left: 50px;
		padding: 10px; /* Отступ текста от края*/
}
</style>

